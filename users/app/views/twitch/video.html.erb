0 , 32 , 48

var video = document.getElementsByTagName("video")[0];
video.addEventListener("timeupdate", function() {
if (video.currentTime >= 0 && video.currentTime <= 3) {
video.playbackRate = .5;
// video.prop('muted', true); //mute
notes.text("Game sense: Too aggresive/Dive. Should have backed after kill. Knew lesh had euls and could cc and kill me.");
}
else if (video.currentTime >= 3 && video.currentTime <= 32){
video.playbackRate = 1;
// video.prop('muted', false); //mute
}
else if (video.currentTime >= 32 && video.currentTime <= 35) {
video.playbackRate = .5;
// video.prop('muted', true); //mute
notes.text("Game sense: Too aggresive/Dive. Understimated TA's damage and died without using spells");
}
else if (video.currentTime >= 35 && video.currentTime <= 48){
video.playbackRate = 1;
// video.prop('muted', false); //mute
}
else if (video.currentTime >= 48 && video.currentTime <= 51) {
video.playbackRate = .5;
notes.text("Game sense: Too aggresive/Dive. Should have know lesh would euls. Could have avoided death by looking at minimap , tping out , stay behind front line");
// video.prop('muted', true); //mute
}
else if (video.currentTime >= 51){
video.playbackRate = 1;
// video.prop('muted', false); //mute
}
}, false);


</script>


